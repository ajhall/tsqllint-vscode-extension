{
    "name": "tsqllint-vs-extension-client",
    "description": "The client for the TSQLLint VS Code Extension",
    "author": "Nathan Boyd",
    "license": "MIT",
    "version": "1.3.0",
    "publisher": "TSQLLint",
    "repository": {
        "type": "git",
        "url": "https://github.com/tsqllint/tsqllint-vscode-extension"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "activationEvents": [
        "onLanguage:sql"
    ],
    "main": "./out/client/src/extension",
    "dependencies": {
        "vscode-languageclient": "^7.0.0",
        "vscode-test": "^0.4.3"
    },
    "scripts": {
        "update-vscode": "node ./node_modules/vscode/bin/install",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/vscode": "^1.55.0"
    }
}
